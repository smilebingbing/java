package com.java.offer;

import java.util.Arrays;

public class Solution12 {

	public boolean isContinuous(int [] numbers){
		if(numbers == null){
			return false;
		}
		Arrays.sort(numbers);
		int number0 =0;
		for(int i=0; i < numbers.length-1; i++){
			if(numbers[i] == 0){
				number0 ++;
			}
		}
		
		int small = number0;
		int big = small + 1;
		while(big < numbers.length ){
			
		}
		
	}
}
