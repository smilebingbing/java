package test;

import java.util.Scanner;

public class ContinusString {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int [] a = new int[n];
		for(int i = 0;i < n;i++){
			a[i] = sc.nextInt();
		}
		for(int i = 0,j = n-1;i < j;i++,j--){
			if(a[i] == a[j]){
				
			}
		}
	}

}
