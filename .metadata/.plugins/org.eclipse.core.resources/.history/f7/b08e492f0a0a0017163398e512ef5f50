package com.java.offer;
//丑数
/*
 * 只包含因子2,3,5的数称为丑数，1是第一个丑数
 */
public class Solution22 {

	public int GetUglyNumber_Solution(int index) {
		
        return 0;
    }
	
	public boolean isUgly(int number){
		while(number % 2 == 0){
			number /=2;
		}
		while(number % 3 ==0){
			number /=2;
		}
		while(number % 5 ==0){
			number /=2;
		}
		
		if(number == 1){
			return true;
		}else{
			return false;
		}
	}
}
