import java.util.List;

public class StudentDao {

	public List<Student> getAll(){
		
		Connection connection =null;
		PreparedStatement preparedStatement =null;
		ResultSet resultSet = null;
		
		try{
			
		}catch(Exception e){
			e.printStackTrace();
		}finally{
			try{
				if(resultSet !=null){
				resultSet.close();
				}
			}catch(SQLException e){
				e.printStackTrace();
			}
			
