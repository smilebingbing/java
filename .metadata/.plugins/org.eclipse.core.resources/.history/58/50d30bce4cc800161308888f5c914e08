import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		while(sc.hasNext()){
			int [][] A=new int [9][9];
			for(int i=0;i<9;i++){
				for(int j=0;j<9;j++){
					A[i][j]=sc.nextInt();
				}
			}
			for(int i=0;i<9;i++){
				for(int j=0;j<9;j++){
					if(j!=8){
						System.out.println(A[i][j]+" ");
					}else{
						System.out.println(A[i][j]);
					}
				}
				if(i!=8){
					System.out.println();
				}
			}
			sudoku(A,0);
		}
		 static boolean Checkrow(int [][] sd,int x,int y,int n){
			for(int i=0;i<9;i++){
				if(i!=x&&sd[i][y]==n)
					return false;
			}
			for(int j=0;j<9;j++){
				if(j!=y&&sd[x][j]==n)
					return false;
			}
			return true;
		}
		 static boolean CheckGrid(int [][] sd,int x,int y,int n)
	}

}
