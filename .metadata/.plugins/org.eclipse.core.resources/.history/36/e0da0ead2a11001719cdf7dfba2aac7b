package test;

import java.util.Scanner;

public class Main2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
			int n = sc.nextInt();
			for(int i = 0; i < n;i++){
				String a = sc.nextLine();
				char [] ch = a.toCharArray();
				
				if(n == 1){
					System.out.println(a.length());
				}
			}
			
		}	
	public static int getSum(int n){
		if(n == 1 || n == 0){
			return 1;
		}else{
			return getSum(n-1)*n;
		}
		
	}
}
