package com.java.offer;

public class GetNextNode {



	public static Node1 getNextNode(Node1 node){
		if(node == null){
			return node;
		}
		if(node.right != null){
			return getNextNode(node.right);
		}
		
	}
	
	public Node1 getMostLeft(Node1 node){
		if(node == null){
			return node;
		}
		while(node.left != null){
			node = node.left;
		}
		return node;
	}

}
