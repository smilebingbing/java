import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		while(sc.hasNext()){
			int [][] A=new int [9][9];
			for(int i=0;i<9;i++){
				for(int j=0;j<9;j++){
					A[i][j]=sc.nextInt();
				}
			}
			for(int i=0;i<9;i++){
				for(int j=0;j<9;j++){
					if(j!=8){
						System.out.println(A[i][j]+" ");
					}else{
						System.out.println(A[i][j]);
					}
				}
				if(i!=8){
					System.out.println();
				}
			}
		}
	}

}
