package com.java.offer;

public class Solution11 {

	public int StrToInt(String str){
		if(str.equals("") || str.length() == 0){
			return 0;
		}
		int q_valid = 0;
		char [] ch = str.toCharArray();
		if(ch[0] == '-'){
			return q_valid=1;
		}
		long sum=0;
		for(int i = q_valid; i<ch.length-1 ;i++){
			if(ch[i] == '+'){
				continue;
			}
			if(ch[i] < 48 || ch[i] > 57){
				return 0;
			}
				sum = sum*10 + ch[i]-48;
		
		}
		return (int) (q_valid == 0 ? sum :sum*(-1));
	}
}
